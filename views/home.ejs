<%- include('partials/header') -%>
    <h1>Home</h1>
    <p><%= startingContent %></p>
    <%posts.forEach(function(element){%>
        <h1><%= element.title%></h1>
        <p style="display:inline"><%= element.content.substring(0, 100) + ' ... ' %></p>
        <a href='/posts/<%= element.title%>'>Read More</a>
    <%})%>
<%- include('partials/footer') -%>


